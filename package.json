{
  "name": "connectedcars",
  "version": "1.0.0",
  "description": "Online Car SuperStore",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
